<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" class="main-page" backgroundSpanUnderStatusBar="true">
    <Page.actionBar>
        <ActionBar title="Title" visibility="collapsed">
        </ActionBar>
    </Page.actionBar>
    <StackLayout orientation="vertical" class="main-container">
        <StackLayout orientation="vertical" class="sub-container">
            <Label text="Library" class="main-title" horizontalAlignment="center"/>
            <StackLayout orientation="vertical" class="content">
                <Button horizontalAlignment="right" class="home-btn" text="Back" tap="{{ nav_back }}"/>
            </StackLayout>
        </StackLayout>
    </StackLayout>
</Page>